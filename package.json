{
  "name": "dailyvibe",
  "version": "1.0.6",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "pre-build": "./scripts/pre-build-check.sh",
    "build:ios": "npm run pre-build && npx eas build --profile production --platform ios",
